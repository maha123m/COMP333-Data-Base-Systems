    <TextField fx:id="searchTextField" layoutX="96.0" layoutY="29.0" prefHeight="25.0" prefWidth="500.0" />

    <TableView fx:id="availabilityTable" layoutX="27.0" layoutY="111.0" prefHeight="235.0" prefWidth="546.0">
        <columns>
            <TableColumn fx:id="trainerID" prefWidth="100" text="trainerID" />
            <TableColumn fx:id="avalableDay" prefWidth="200" text="availableDay" />
            <TableColumn fx:id="avalableTime" prefWidth="200" text="availableTime" />
        </columns>
    </TableView>
    <Button fx:id="reserveButton" disable="true" layoutX="27.0" layoutY="370.0" onAction="#handleReserveButton" prefHeight="39.0" prefWidth="115.0" text="Reserve" />
   <Button fx:id="Home" layoutX="570.0" layoutY="377.0" mnemonicParsing="false" onAction="#goTomainInterface" text="Home" />


